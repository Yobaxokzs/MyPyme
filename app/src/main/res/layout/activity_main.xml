<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Inicio">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#EDF2F3"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="106dp">>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="206dp"
                android:layout_height="177dp"
                android:layout_marginTop="44dp"
                android:onClick="Producto"
                app:layout_constraintEnd_toStartOf="@+id/imageView11"
                app:layout_constraintHorizontal_bias="0.28"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView2"
                app:srcCompat="@drawable/producto1" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="198dp"
                android:layout_height="177dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="16dp"
                android:onClick="Proveedor"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView2"
                app:srcCompat="@drawable/proveedoresf" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="205dp"
                android:layout_height="176dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="36dp"
                android:onClick="Clientes"
                app:layout_constraintEnd_toStartOf="@+id/imageView13"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView10"
                app:srcCompat="@drawable/clientesf" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="199dp"
                android:layout_height="175dp"
                android:layout_marginTop="48dp"
                android:layout_marginEnd="12dp"
                android:onClick="Pedidos"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView11"
                app:srcCompat="@drawable/pedidosf" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="204dp"
                android:layout_height="177dp"
                android:layout_marginTop="448dp"
                android:onClick="Inventario"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.534"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView2"
                app:srcCompat="@drawable/inventoryf" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="325dp"
                android:layout_height="176dp"
                android:layout_marginTop="32dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/logo_small" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>